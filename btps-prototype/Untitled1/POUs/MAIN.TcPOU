		LAS:BTS:MCS2:01:m1 (ioc-las-bts-mcs1)
		LAS:BTS:MCS2:01:m4 (ioc-las-bts-mcs1)
		LAS:BTS:MCS2:01:m7 (ioc-las-bts-mcs1)
	*)
	{attribute 'pytmc' := '
		pv: @(MOTOR_PREFIX):Chk:B1Lin
