    <Implementation>
      <ST><![CDATA[fbBay1Linear(bEnable:=TRUE);
fbBay3Linear(bEnable:=TRUE);
fbBay4Linear(bEnable:=TRUE);

fbBay1Rotary(bEnable:=TRUE);
