record(bi, "$(PREFIX)LCLSGeneral:GlobalLogTrickleTrip_RBV") {
  field(DESC, "Tripped by overall log count")
  field(SCAN, "I/O Intr")
  field(PINI, "1")
  field(TSE, "-2")
  field(DTYP, "asynInt32")
